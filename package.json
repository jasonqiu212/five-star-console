{
  "name": "admin-hub",
  "private": true,
  "version": "1.0.0",
  "description": "Admin Hub Monorepo",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:frontend": "pnpm --filter @five-star-auto-leather/admin-hub-frontend dev",
    "dev:backend": "pnpm --filter @five-star-auto-leather/admin-hub-backend dev",
    "build:frontend": "pnpm --filter @five-star-auto-leather/admin-hub-frontend build",
    "build:backend": "pnpm --filter @five-star-auto-leather/admin-hub-backend build",
    "build": "pnpm -r build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
